<?xml version='1.0' encoding='utf-8'?>
<widget id="app.ideapad.id" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ideapad</name>
    <description>App for Ideas and Notes</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/android/36-ldpi.png" />
        <icon density="xxxhdpi" src="res/android/192-xxxhdpi.png" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
